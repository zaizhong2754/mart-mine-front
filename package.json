    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
